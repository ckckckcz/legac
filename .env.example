# PostgreSQL Database
DATABASE_URL="postgresql://USERNAME:PASSWORD@HOST:PORT/DATABASE?sslmode=require"

# GitHub OAuth Configuration
# Get these values from https://github.com/settings/developers
GITHUB_ID="your_github_oauth_app_id_here"
GITHUB_SECRET="your_github_oauth_app_secret_here"

# NextAuth Secret for session encryption
# Generate a secure random string using: openssl rand -base64 32
AUTH_SECRET="generate_a_secure_random_string_here"

# Optional: GitHub Personal Access Token for GitHub API calls
# Get this from https://github.com/settings/tokens
REACT_APP_GITHUB_TOKEN="your_github_token_here"
